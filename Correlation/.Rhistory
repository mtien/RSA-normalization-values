pred<-function(n,k){
f<-function(x){x^(k+10-1)*(1-x)^(n-k+10-1)}
val<-integrate(f, lower=0, upper=1)[1]$value*gamma(k+10+n-k+10)*(1/gamma(n-k+10))*(1/gamma(k+10))
return val
}
